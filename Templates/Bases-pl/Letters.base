filters:
  and:
    - categories == ["[[Letters]]"]
views:
  - type: table
    name: Table
    filters:
      and:
        - '!file.name.contains("Template")'
    order:
      - file.name
      - categories
      - type
      - topics
      - subject
      - created
      - last
    sort:
      - property: last
        direction: ASC
      - property: tags
        direction: ASC
    columnSize:
      file.name: 215
      note.categories: 107
      note.topics: 119
